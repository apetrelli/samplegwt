<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:w="urn:import:com.github.apetrelli.gwtintegration.cellview.client.widget">
	<ui:style>
		
	</ui:style>
	<ui:with type="${package}.module.client.i18n.MainMessages" field="messages" />
	<g:FlowPanel>
		<w:SimplePagedTable ui:field="dataTable" />
		<g:Button text="{messages.add}" ui:field="add" />
	</g:FlowPanel>
</ui:UiBinder> 