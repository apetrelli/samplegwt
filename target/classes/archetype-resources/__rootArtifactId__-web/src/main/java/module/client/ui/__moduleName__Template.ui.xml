#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.resized {
			height: 100%
		}
	</ui:style>
	<g:DockLayoutPanel>
		<g:north size="100.0">
			<g:FlowPanel>
				<g:Label text="Sample application"/>
			</g:FlowPanel>
		</g:north>
		<g:west size="100.0">
			<g:FlowPanel>
				<g:Hyperlink text="Persons" targetHistoryToken="ListPersonsPlace:" />
			</g:FlowPanel>
		</g:west>
		<g:center>
			<g:SimplePanel ui:field="body"/>
		</g:center>
		<g:south size="100.0">
			<g:FlowPanel>
				<g:Label text="Footer"/>
			</g:FlowPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder> 